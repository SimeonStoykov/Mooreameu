@model IEnumerable<Mooreameu.Model.Contest>
@{
    ViewBag.Title = "Home";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">Team Mooremeu</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Newest contests</h2>
        <ul>
            @{
                foreach (var contest in Model)
                {
                    @:<li><h2>@contest.Title</h2>
                           <p>@contest.Description</p>      
                      @:</li>
                }
            }
        </ul>

        <p><a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkId=301865">View all &raquo;</a></p>
    </div>
</div>